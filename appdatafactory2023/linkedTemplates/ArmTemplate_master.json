{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "appdatafactory2023"
		},
		"DemoVM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DemoVM'"
		},
		"Synapse_staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Synapse_staging'"
		},
		"dbsrv203_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dbsrv203'"
		},
		"newpool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newpool'"
		},
		"sadatalake2000_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sadatalake2000'"
		},
		"DemoVM_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\localhost\\nginx-1.22.1\\nginx-1.22.1\\logs"
		},
		"DemoVM_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"sadatalake2000_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sadatalake2000.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "appdatafactory2023_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Synapse_staging_connectionString": {
						"value": "[parameters('Synapse_staging_connectionString')]"
					},
					"dbsrv203_connectionString": {
						"value": "[parameters('dbsrv203_connectionString')]"
					},
					"newpool_connectionString": {
						"value": "[parameters('newpool_connectionString')]"
					},
					"sadatalake2000_accountKey": {
						"value": "[parameters('sadatalake2000_accountKey')]"
					},
					"sadatalake2000_properties_typeProperties_url": {
						"value": "[parameters('sadatalake2000_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "appdatafactory2023_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appdatafactory2023_ArmTemplate_0')]"
			]
		},
		{
			"name": "appdatafactory2023_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DemoVM_password": {
						"value": "[parameters('DemoVM_password')]"
					},
					"DemoVM_properties_typeProperties_host": {
						"value": "[parameters('DemoVM_properties_typeProperties_host')]"
					},
					"DemoVM_properties_typeProperties_userId": {
						"value": "[parameters('DemoVM_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appdatafactory2023_ArmTemplate_1')]"
			]
		},
		{
			"name": "appdatafactory2023_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appdatafactory2023_ArmTemplate_2')]"
			]
		}
	]
}